{
  "extends": ["next/core-web-vitals", "airbnb", "airbnb/hooks"],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-no-useless-fragment": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": ["error", { "code": 400 }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "path": ["./src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
